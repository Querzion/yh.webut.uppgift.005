.DS_Store
.AppleDouble
.LSOverride
._*
Thumbs.db
ehthumbs.db
Desktop.ini
$RECYCLE.BIN/
*.lnk

# Ignore Git-specific files
.git/
.gitmodules
.gitattributes
.gitkeep
.gitconfig
.git-lfs
*.orig

